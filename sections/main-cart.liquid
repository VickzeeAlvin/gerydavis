<div class="page-width page-width--cart page-content tabs_details tab-active" id="your_cart">

  {%- render 'breadcrumbs' -%}

  {%- if cart.item_count == 0 -%}
  <header class="section-header text-center{% if cart.item_count == 0 %} section-header--404{% endif %}">
    <h1 class="section-header__title">{{ 'cart.general.title' | t }}</h1>
    <div class="rte text-spacing">
      <p>{{ 'cart.general.empty' | t }}</p>
      <hr class="hr--clear hr--small">
      <p>
        <a href="{{ routes.root_url }}" class="btn">{{ 'cart.general.continue_shopping' | t }}</a>
      </p>
    </div>
  </header>
  {%- endif -%}

  {%- if cart.item_count > 0 -%}
  <form action="{{ routes.cart_url }}" method="post" novalidate data-location="page" id="CartPageForm">
    <div class="cart__page">
      <div class="cart__page-col">
        {%comment%}<h1 class="section-header__title">{{ 'cart.general.title' | t }}</h1>{%endcomment%}
        <div class="cart-table">
          <table class="cart-items">
            <thead>
              <tr>
                <th colspan="2"></th>
                <th colspan="1">Price</th>
                <th colspan="1">Quantity</th>
                <th colspan="1"></th>
              </tr>
            </thead>
          </table>
          <div data-products>
            {% for item in cart.items %}
            {%- comment -%}Bold:PO 2.0 - Hybrid Input{%- endcomment -%}
            {%- if item.properties._boldBuilderId  and item.properties._boldVariantIds == null -%}
            <input type="hidden" name="updates[]" id="updates_{{- item.key -}}" class="bold_hidden_input cart__product-qty" value="{{- item.quantity -}}" min="0" data-id="{{- item.key -}}">
            {%- continue -%}
            {%- endif -%}
            {%- render 'cart-item', product: item -%}
            {% endfor %}
          </div>
        </div>
        <div class="charity__row">
          <i data-govx-id></i>
        </div>
      </div>

      <div class="cart__page-col cart__sidebar">
        <h3 class="section-header__title">CART SUMMARY</h3>

        {% if settings.cart_notes_enable %}
        <div class="cart__item-row">
          <label for="CartNote" class="add-note">
            <span class="cart__note-text">
              <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-pencil mobile" viewBox="0 0 64 64"><defs><style>.cls-1,.cls-2{fill:none;stroke:#000;stroke-width:2px}.cls-1{stroke-miterlimit:10}</style></defs><path class="cls-1" d="M52 33.96V58H8V14h23.7"/><path class="cls-1" d="M18 47l13-5 24-24-8-8-24 24-5 13zM23 34l8 8"/><path id="svg_2" data-name="svg 2" class="cls-2" d="M47 10l1-1a5.44 5.44 0 0 1 7.82.18A5.52 5.52 0 0 1 56 17l-1 1"/></svg>
              {{ 'cart.general.note' | t }}
            </span>
            <span class="note-icon note-icon--open" aria-hidden="true">
               <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-pencil desktop" viewBox="0 0 64 64"><defs><style>.cls-1,.cls-2{fill:none;stroke:#000;stroke-width:2px}.cls-1{stroke-miterlimit:10}</style></defs><path class="cls-1" d="M52 33.96V58H8V14h23.7"/><path class="cls-1" d="M18 47l13-5 24-24-8-8-24 24-5 13zM23 34l8 8"/><path id="svg_2" data-name="svg 2" class="cls-2" d="M47 10l1-1a5.44 5.44 0 0 1 7.82.18A5.52 5.52 0 0 1 56 17l-1 1"/></svg>
             <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right mobile" viewBox="0 0 284.49 498.98"><path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path></svg>
            </span>
            <span class="note-icon note-icon--close desktop">
              <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close desktop" viewBox="0 0 64 64"><defs><style>.cls-1{fill:none;stroke:#000;stroke-miterlimit:10;stroke-width:2px}</style></defs><path class="cls-1" d="M19 17.61l27.12 27.13m0-27.13L19 44.74"/></svg>
              <span class="icon__fallback-text">{{ 'general.accessibility.close' | t }}</span>
            </span>
          </label>
          <textarea name="note" class="input-full cart__note hide" id="CartNote">{{ cart.note }}</textarea>
        </div>
        {% endif %}
        <div class="mobile_route text_line"><p>This premium service covers your package if it is lost, damaged or stolen.</p></div>
        <div class="mobile_route">
          <div class="route-div"></div>
        </div>
        <div class="mobile_govx">
          <i data-govx-id></i>
        </div>
        <div class="mobike_pledgeling">
           <div data-round-up-via-pledgeling="7253308571819"></div>
        </div>
        <div class="checkout_bttn">
        <button type="submit" name="checkout" data-terms-required="{{ settings.cart_terms_conditions_enable }}" class="btn cart__checkout">
          {{ 'cart.general.checkout' | t }}
        </button>
        </div>
        <div class="cart__item-sub cart__item-row cart__item--subtotal">
          <div>{{ 'cart.general.subtotal' | t }}</div>
          <span class="Bold-theme-hook-DO-NOT-DELETE bold_cart_total" style="display:none !important;"></span>
          <div data-subtotal>
            {%- render 'price' with cart.total_price -%}
          </div>
        </div>

        <div class="cart__item-row text-right">
          <i>{{ 'cart.general.shipping_at_checkout' | t }}</i>
        </div>

        <div class="cart__item-row give__grow">
          <div data-round-up-via-pledgeling="7253308571819"></div>
        </div>

        <div class="cart__item-row product_policy">
          <ul class="product-text"> 
            <li class="pro_title"> 
              <span class="title_icon"> <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16" viewBox="0 0 16 16" fill="none"><rect width="16" height="16" fill="url(#icon_tick)"></rect><defs><pattern id="icon_tick" patternContentUnits="objectBoundingBox" width="1" height="1"><use xlink:href="#image0_1210:47" transform="scale(0.03125)"></use></pattern><image id="image0_1210:47" width="32" height="32" xlink:href="data:image/png;base64,"></image></defs></svg> </span> 
              <span class="title_text">Orders $99+ ship free within the contiguous United States. Most orders placed by 1:00pm Central ship the same day.</span> 
            </li>
            <li class="pro_title"> 
              <span class="title_icon"> <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="18" height="14" viewBox="0 0 18 14" fill="none"><rect width="18" height="13.3953" fill="url(#icon_return)"></rect><defs><pattern id="icon_return" patternContentUnits="objectBoundingBox" width="1" height="1"><use xlink:href="#image0_1209:46" transform="scale(0.0232558 0.03125)"></use></pattern><image id="image0_1209:46" width="43" height="32" xlink:href="data:image/png;base64,"></image></defs></svg> </span> 
              <span class="title_text">Most orders can be returned within 30 days or exchanged within 60 days for free</span> 
            </li>
          </ul>
        </div>

        <div data-discounts>
          {% if cart.cart_level_discount_applications != blank %}
          <div class="cart__discounts text-right">
            <div>
              {% for cart_discount in cart.cart_level_discount_applications %}
              {%- assign savings = cart_discount.total_allocated_amount | money -%}
              <div class="cart__discount">
                {{ 'cart.general.savings_html' | t: saved_amount: savings }}
                ({{ cart_discount.title }})
              </div>
              {% endfor %}
            </div>
          </div>
          {% endif %}
        </div>

        {% if settings.cart_terms_conditions_enable %}
        <div class="cart__item-row cart__terms">
          <input type="checkbox" id="CartTerms" class="cart__terms-checkbox">
          <label for="CartTerms" class="text-label">
            <small>
              {% if settings.cart_terms_conditions_page != blank %}
              {{ 'cart.general.terms_html' | t: url: settings.cart_terms_conditions_page.url }}
              {% else %}
              {{ 'cart.general.terms' | t }}
              {% endif %}
            </small>
          </label>
        </div>
        {% endif %}

        {% unless cart.item_count == 0 %}
        	{% render 'shipping-rates-calculator' %}
        {% endunless %}

        <div class="cart__item-row">
          <div class="route-div"></div>
        </div>

        <div class="shipping__calculator"></div>

        <div class="cart__item-row cart__checkout-wrapper">
          <button type="submit" name="checkout" data-terms-required="{{ settings.cart_terms_conditions_enable }}" class="btn cart__checkout">
            {{ 'cart.general.checkout' | t }}
          </button>

          {% if additional_checkout_buttons and settings.cart_additional_buttons %}
          <div class="additional-checkout-buttons">{{ content_for_additional_checkout_buttons }}</div>
          {% endif %}

          <a href="{{ routes.all_products_collection_url }}" class="btn btn--secondary cart__continue">
            {{ 'cart.general.continue_shopping' | t }}
          </a>
        </div>

      </div>
    </div>
  </form>
  {%- endif -%}
</div>


<div class="cart_mobile_sticky" >
  <div class="cart_mobile_sticky_left">
    <div>
      <span class="sub_total">
        {{ 'cart.general.subtotal' | t }}:
      </span>
      <span class="sub_total_amount">
        <span class="Bold-theme-hook-DO-NOT-DELETE bold_cart_total" style="display:none !important;"></span>
        <div data-subtotal>
          {%- render 'price' with cart.total_price -%}
        </div>
      </span>
    </div>
    <p>taxes and discounts are calculated at checkout</p>
  </div>
  <div class="cart_mobile_sticky_right">
    <button type="submit" name="checkout" data-terms-required="{{ settings.cart_terms_conditions_enable }}" class="btn cart__checkout custon_checkout">
      {{ 'cart.general.checkout' | t }}
    </button>
  </div>
</div>

{% schema %}
{
  "name": "t:sections.main-cart.name"
}
{% endschema %}


<script>
  $(document).on('click', '.js-qty__wrapper', function(){
    $('[name="checkout"]').attr("disabled", true);
    setTimeout(function() {
      $('[name="checkout"]').attr("disabled", false);
      var subTotal = $('.cart__sidebar .cart__item--subtotal div[data-subtotal]').html();
      $('.sub_total_amount div[data-subtotal]').html(subTotal);
    },2000);
  });

  $(document).on('click', '.cart__remove', function(){
    $('[name="checkout"]').attr("disabled", true);
  });
</script>